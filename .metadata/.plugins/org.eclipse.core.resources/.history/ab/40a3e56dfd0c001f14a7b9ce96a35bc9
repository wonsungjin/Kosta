package com.kosta.sample.qulz1;

class Score
{
	private int [] scores = new int[3];
	public Score()
	{
		System.out.print("국어" +"영어"+"수학\n");
		System.out.print("---" +"---"+"---\n");
		new Score(scores[0],scores[1],scores[2]);
	}
	public Score(int a,int b,int c)
	{
		System.out.print(a+" " + b+" " +c);
	}
	public void chageScore(int a,int b)
	{
		scores[a]=b;
	}
	
}
public class Sukang extends Score {
	
	
	public Sukang(int a,int b, int c) 
	{
		super.chageScore(0,a);
		super.chageScore(1,b);
		super.chageScore(2,c);
	}
	
	public static void main(String[] args) {
		new Sukang(55,42,35);

	}

}
